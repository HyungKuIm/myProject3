<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>user list</title>
</head>
<body>
    <table class="table">
        <% for(var user of content) { %>
        <tr>
            <th><%= user._id %></th>
            <td><%= user.name %></td>
            <td><%= user.age %></td>
        </tr>
        <% } %>
    </table>
</body>
</html>